  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "start:api": "firebase serve --port 3000 --only functions,hosting",
    "build": "webpack --mode=development --config webpack.prod.js",
    "firebase:deploy:hosting": "npm run build&&firebase deploy --only hosting -m \"release\"",
    "firebase:deploy:functions": "npm run build&&firebase deploy --only functions -m \"release\"",
    "firebase:deploy:all": "npm run build&&firebase deploy -m \"release\"",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix"
  },
